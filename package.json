{
  "name": "plexus2",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "bun --watch index.ts --config ./config/plexus.yaml",
    "dev:frontend": "bun src/index.html --host 0.0.0.0",
    "test": "bun test",
    "typecheck": "bunx tsc --noEmit",
    "start": "NODE_ENV=production bun index.js --config ./config/plexus.yaml",
    "build": "bun build --target=bun --production --outdir=dist ./index.ts",
    "build:frontend": "bun build ./src/index.html --outdir ./dist/frontend --minify",
    "compile:macos": "bun compile.ts --target bun-darwin-arm64 --outfile dist/plexus2-macos",
    "compile:linux": "bun compile.ts --target bun-linux-x64-modern --outfile dist/plexus2-linux-x64",
    "compile:windows": "bun compile.ts --target bun-windows-x64 --outfile dist/plexus2-windows-x64.exe",
    "compile:linux-arm64": "bun compile.ts --target bun-linux-arm64 --outfile dist/plexus2-linux-arm64",
    "compile:all": "bun run compile:macos && bun run compile:linux && bun run compile:windows && bun run compile:linux-arm64"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@google/genai": "^1.35.0",
    "@microsoft/fetch-event-source": "^2.0.1",
    "@monaco-editor/react": "^4.7.0",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "emblor": "^1.4.8",
    "eventsource-encoder": "^1.0.1",
    "eventsource-parser": "^3.0.6",
    "lucide-react": "^0.562.0",
    "next-themes": "^0.4.6",
    "openai": "^6.16.0",
    "openapi-fetch": "^0.15.0",
    "openapi-typescript": "^7.10.1",
    "pino": "^10.1.1",
    "pino-pretty": "^13.1.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-easy-sort": "^1.8.0",
    "react-router-dom": "^7.12.0",
    "recharts": "^3.6.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^13.0.0",
    "yaml": "^2.8.2",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@radix-ui/react-toast": "^1.2.15",
    "@types/bun": "latest",
    "@types/react": "^19.0.6",
    "@types/react-dom": "^19.0.2",
    "@types/uuid": "^11.0.0",
    "bun-plugin-tailwind": "latest",
    "bun-types": "^1.3.5",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3"
  }
}
